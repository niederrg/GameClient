<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gameclient.FXMLLobbyController">
   <children>
      <Label fx:id="lobbyLabel" alignment="CENTER" layoutX="194.0" layoutY="23.0" text="Game Lobby" textAlignment="CENTER" AnchorPane.topAnchor="32.0">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <Rectangle fx:id="player1Color" arcHeight="5.0" arcWidth="5.0" fill="#c5c8ca" height="300.0" layoutY="91.0" stroke="BLACK" strokeType="INSIDE" width="300.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" />
      <Rectangle fx:id="player2Color" arcHeight="5.0" arcWidth="5.0" fill="#cfcecd" height="300.0" layoutX="308.0" layoutY="101.0" stroke="BLACK" strokeType="INSIDE" width="300.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" />
      <Label fx:id="player1" layoutX="108.0" layoutY="110.0" text="Player 1" underline="true">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label fx:id="player2" layoutX="408.0" layoutY="110.0" text="Player 2" underline="true">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="player1Ready" layoutX="114.0" layoutY="346.0" mnemonicParsing="false" onAction="#setP1Ready" text="Ready" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="400.0">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="player2Ready" layoutX="124.0" layoutY="356.0" mnemonicParsing="false" onAction="#setP2Ready" text="Ready" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="400.0" AnchorPane.rightAnchor="100.0">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
